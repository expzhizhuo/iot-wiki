in userLinks\">\r\n        <a-sub-menu v-if=\"item.type === 'links'\" :key=\"link(item.link)\">\r\n          <span slot=\"title\">\r\n            {{ item.text }}\r\n          </span>\r\n          <template v-for=\"(subItem, index) in item.items\">\r\n            <a-menu-item-group\r\n              :title=\"subItem.text\"\r\n              v-if=\"subItem.type === 'links'\"\r\n              :key=\"link(subItem.link) || index\"\r\n            >\r\n              <a-menu-item\r\n                v-for=\"childSubItem in subItem.items\"\r\n                :key=\"link(childSubItem.link)\"\r\n              >\r\n                <a\r\n                  v-if=\"isExtlink(childSubItem.link)\"\r\n                  :href=\"childSubItem.link\"\r\n                  target=\"_blank\"\r\n                  rel=\"noopener noreferrer\"\r\n                >\r\n                  {{ childSubItem.text }}\r\n                  <a-icon type=\"link\" />\r\n                </a>\r\n                <RouterLink v-else :to=\"link(childSubItem.link)\">\r\n                  {{ childSubItem.text }}\r\n                </RouterLink>\r\n              </a-menu-item>\r\n            </a-menu-item-group>\r\n            <a-menu-item :key=\"link(subItem.link)\" v-else>\r\n              <a\r\n                v-if=\"isExtlink(subItem.link)\"\r\n                :href=\"subItem.link\"\r\n                target=\"_blank\"\r\n                rel=\"noopener noreferrer\"\r\n              >\r\n                {{ subItem.text }}\r\n                <a-icon type=\"link\" />\r\n              </a>\r\n              <RouterLink v-else :to=\"link(subItem.link)\">\r\n                {{ subItem.text }}\r\n              </RouterLink>\r\n            </a-menu-item>\r\n          </template>\r\n        </a-sub-menu>\r\n        <a-menu-item :key=\"link(item.link)\" v-else>\r\n          <a\r\n            v-if=\"isExtlink(item.link)\"\r\n            :href=\"link(item.link)\"\r\n            target=\"_blank\"\r\n          >\r\n            {{ item.text }}\r\n            <a-icon type=\"link\" />\r\n          </a>\r\n          <RouterLink v-else :to=\"link(item.link)\">\r\n            {{ item.text }}\r\n          </RouterLink>\r\n        </a-menu-item>\r\n      </template>\r\n    </a-menu>\r\n    <ul class=\"extra-group\">\r\n      <li v-if=\"repoLink\">\r\n        <a-tooltip placement=\"bottom\">\r\n          <template slot=\"title\">\r\n            <span style=\"text-transform:capitalize;\">{{ repoLabel }}</span>\r\n          </template>\r\n          <a\r\n            :href=\"repoLink\"\r\n            class=\"repo-link\"\r\n            target=\"_blank\"\r\n            rel=\"noopener noreferrer\"\r\n          >\r\n            <a-icon :type=\"repoLabel\" />\r\n          </a>\r\n        </a-tooltip>\r\n      </li>\r\n\r\n      <li v-if=\"isDarkMode\">\r\n        <ThemeSwitch />\r\n      </li>\r\n    </ul>\r\n  </nav>\r\n</template>\r\n\r\n<script>\r\nimport { resolveNavLinkItem, ensureExt } from '../util';\r\nimport ThemeSwitch from '@theme/components/ThemeSwitch.vue';\r\n\r\nexport default {\r\n  name: 'NavLinks',\r\n  components: {\r\n    ThemeSwitch\r\n  },\r\n\r\n  data() {\r\n    return {\r\n      currentStyle: this.$store.state.global.navStyle,\r\n      routesPath: ''\r\n    };\r\n  },\r\n  created() {\r\n    this.routesPath = JSON.stringify(\r\n      this.$themeConfig.locales\r\n        ? this.$themeConfig.locales[this.$localePath].nav\r\n        : this.$themeConfig.nav\r\n    );\r\n  },\r\n  methods: {\r\n    isExtlink(path) {\r\n      const Reg = /^(?:http(s)?:\\/\\/)?[\\w.-]+(?:\\.[\\w\\.-]+)+[\\w\\-\\._~:/?#[\\]@!\\$&'\\*\\+,;=.]+$/;\r\n      return Reg.test(path);\r\n    },\r\n    link(url) {\r\n      url = typeof url === 'undefined' ? '' : url;\r\n      let _url = ensureExt(url);\r\n      _url = _url.length === 5 && _url === '.html' ? this.geneKey() : _url;\r\n      return _url;\r\n    },\r\n    geneKey() {\r\n      return (\r\n        Math.random()\r\n          .toString(36)\r\n          .substr(2, 10) + new Date().getTime()\r\n      );\r\n    },\r\n    currentPath(path) {\r\n      const array = this.routesPath;\r\n      if (array.indexOf(path) != -1) {\r\n        return path;\r\n      } else {\r\n        let _suffix = path.slice(-5);\r\n        if (_suffix === '.html') {\r\n          let _path = path.replace('.html', '');\r\n          if (array.indexOf(_path) != -1) {\r\n            return path;\r\n          } else {\r\n            let _path = path.replace(/[^\\/]+$/, '');\r\n            return _path;\r\n          }\r\n        } else {\r\n          let _path = path.replace(/[^\\/]+$/, '');\r\n          return _path;\r\n        }\r\n      }\r\n    },\r\n    activePage(path) {\r\n      const array = this.routesPath;\r\n      if (array.indexOf('activeMatch') == -1) {\r\n        return this.currentPath(path);\r\n      } else {\r\n        for (let i = 0; i < this.userLinks.length; i++) {\r\n          if (typeof this.userLinks[i].activeMatch === 'undefined') {\r\n            continue;\r\n          } else {\r\n            if (new RegExp(this.userLinks[i].activeMatch).test(path)) {\r\n              return this.userLinks[i].link;\r\n            } else {\r\n              return this.currentPath(path);\r\n            }\r\n          }\r\n        }\r\n      }\r\n    }\r\n  },\r\n  computed: {\r\n    currentPage() {\r\n      return [this.activePage(this.$page.path)];\r\n    },\r\n\r\n    userNav() {\r\n      return this.$themeLocaleConfig.nav || this.$site.themeConfig.nav || [];\r\n    },\r\n\r\n    nav() {\r\n      const { locales } = this.$site;\r\n      if (locales && Object.keys(locales).length > 1) {\r\n        const currentLink = this.$page.path;\r\n        const routes = this.$router.options.routes;\r\n        const themeLocales = this.$site.themeConfig.locales || {};\r\n        const languageDropdown = {\r\n          text: this.$themeLocaleConfig.selectText || 'Languages',\r\n          ariaLabel: this.$themeLocaleConfig.ariaLabel || 'Select language',\r\n          items: Object.keys(locales).map(path => {\r\n            const locale = locales[path];\r\n            const text =\r\n              (themeLocales[path] && themeLocales[path].label) || locale.lang;\r\n            let link;\r\n            // Stay on the current page\r\n            if (locale.lang === this.$lang) {\r\n              link = currentLink;\r\n            } else {\r\n              // Try to stay on the same page\r\n              link = currentLink.replace(this.$localeConfig.path, path);\r\n              // fallback to homepage\r\n              if (!routes.some(route => route.path === link)) {\r\n                link = path;\r\n              }\r\n            }\r\n            return { text, link };\r\n          })\r\n        };\r\n        return [...this.userNav, languageDropdown];\r\n      }\r\n      return this.userNav;\r\n    },\r\n\r\n    userLinks() {\r\n      const curNav = (this.nav || []).map(link => {\r\n        return Object.assign(resolveNavLinkItem(link), {\r\n          items: (link.items || []).map(resolveNavLinkItem)\r\n        });\r\n      });\r\n      this.routesPath = JSON.stringify(curNav);\r\n      return curNav;\r\n    },\r\n\r\n    repoLink() {\r\n      const { repo } = this.$site.themeConfig;\r\n      if (repo) {\r\n        return /^https?:/.test(repo) ? repo : `https://github.com/${repo}`;\r\n      }\r\n      return null;\r\n    },\r\n\r\n    repoLabel() {\r\n      if (!this.repoLink) return;\r\n      if (this.$site.themeConfig.repoLabel) {\r\n        return this.$site.themeConfig.repoLabel;\r\n      }\r\n\r\n      const repoHost = this.repoLink.match(/^https?:\\/\\/[^/]+/)[0];\r\n      const platforms = ['github', 'gitlab'];\r\n      for (let i = 0; i < platforms.length; i++) {\r\n        const platform = platforms[i];\r\n        if (new RegExp(platform, 'i').test(repoHost)) {\r\n          return platform;\r\n        }\r\n      }\r\n\r\n      return 'global';\r\n    },\r\n    isDarkMode() {\r\n      return this.$themeConfig.isDarkMode || false;\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n@import '../styles/palette.less';\r\n\r\n.nav-links {\r\n  display: flex;\r\n  align-items: center;\r\n  // float: right;\r\n  margin-right: 2rem;\r\n  font-size: 1rem;\r\n\r\n  .repo-link,\r\n  .theme-switch {\r\n    color: inherit;\r\n    font-size: 1.2rem;\r\n    padding: 0 6px;\r\n    border-radius: 4px;\r\n    transition: background-color 0.3s cubic-bezier(0.215, 0.61, 0.355, 1);\r\n\r\n    &:hover {\r\n      background-color: #f2f3f5;\r\n    }\r\n  }\r\n  .repo-link {\r\n    padding: 1px 7px;\r\n  }\r\n\r\n  .extra-group {\r\n    display: flex;\r\n    align-items: center;\r\n    list-style-type: none;\r\n    // line-height: inherit;\r\n    padding: 0;\r\n    margin: 0;\r\n\r\n    li {\r\n      margin: 0 0.3125rem;\r\n    }\r\n  }\r\n}\r\n\r\n@media (max-width: @MQMobile) {\r\n  .nav-links {\r\n    float: none;\r\n    margin-right: 0;\r\n\r\n    .extra-group {\r\n      display: none;\r\n    }\r\n    // .repo-link {\r\n    //   display: none;\r\n    // }\r\n    .ant-menu-inline,\r\n    .ant-menu-vertical,\r\n    .ant-menu-vertical-left {\r\n      border: none;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}